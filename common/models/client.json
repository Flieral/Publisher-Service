{
  "name": "client",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "companyName": {
      "type": "string",
      "required": true,
      "index": true
    },
    "password": {
      "type": "string",
      "required": true,
      "index": true,
      "min": 6
    },
    "email": {
      "type": "string",
      "required": true,
      "index": true
    },
    "username": {
      "type": "string",
      "required": true,
      "index": true
    },
    "time": {
      "type": "number",
      "required": true,
      "index": true
    },
    "registrationCountry": {
      "type": "string",
      "required": true,
      "index": true
    },
    "registrationIPAddress": {
      "type": "string",
      "required": true,
      "index": true
    },
    "registrationStatus": {
      "type": "string",
      "required": false,
      "index": false
    },
    "webhookURL": {
      "type": "string",
      "required": false,
      "index": false
    },
    "message": {
      "type": "string",
      "required": false,
      "index": false
    }
  },
  "validations": [],
  "relations": {
    "account": {
      "type": "embedsOne",
      "model": "account",
      "property": "accountModel",
      "options": {
        "validate": true,
        "forceId": true,
        "persistent": true
      }
    },
    "publishers": {
      "type": "embedsMany",
      "model": "publisher",
      "property": "publisherList",
      "options": {
        "validate": true,
        "forceId": true,
        "persistent": true
      }
    }
  },
  "acls": [{
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "founder",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "changePassword"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__account"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__create__account"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__update__account"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__destroy__account"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__publishers"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__create__publishers"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__update__publishers"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__destroy__publishers"
    }
  ],
  "hidden": [
    "id",
    "realm",
    "message",
    "webhookURL",
    "emailVerified",
    "registrationStatus"
  ],
  "methods": {}
}
