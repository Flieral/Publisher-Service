{
  "name": "client",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "companyName": {
      "type": "string",
      "required": true,
      "index": true
    },
    "password": {
      "type": "string",
      "required": true,
      "index": true,
      "min": 6
    },
    "email": {
      "type": "string",
      "required": true,
      "index": true
    },
    "username": {
      "type": "string",
      "required": true,
      "index": true
    },
    "time": {
      "type": "number",
      "required": true,
      "index": true
    },
    "registrationCountry": {
      "type": "string",
      "required": true,
      "index": true
    },
    "registrationIPAddress": {
      "type": "string",
      "required": true,
      "index": true
    }
  },
  "validations": [],
  "relations": {
    "publisherAccount": {
      "type": "embedsOne",
      "model": "publisherAccount",
      "property": "publisherAccountModel",
      "options": {
        "validate": true,
        "forceId": true,
        "persistent": true
      }
    },
    "publishers": {
      "type": "embedsMany",
      "model": "publisher",
      "property": "publisherList",
      "options": {
        "validate": true,
        "forceId": true,
        "persistent": true
      }
    }
  },
  "acls": [{
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "founder",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "changePassword"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "Deny",
      "property": "replaceById"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__publisherAccount"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "DENY",
      "property": "__create__publisherAccount"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "DENY",
      "property": "__update__publisherAccount"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__destroy__publisherAccount"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__publishers"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__create__publishers"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "DENY",
      "property": "__updateById__publishers"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__destroyById__publishers"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__findById__publishers"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__count__publishers"
    }
  ],
  "hidden": [
    "realm",
    "emailVerified",
    "announcerAccountModel",
    "publisherAccountModel",
    "publisherList"
  ],
  "methods": {}
}
